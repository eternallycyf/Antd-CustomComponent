{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "proseWrap": "preserve",
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-packagejson"
  ]
}